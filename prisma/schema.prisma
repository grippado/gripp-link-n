// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../db/gripp-link.db"
}

model Links {
  id      Int     @id @default(autoincrement())
  url     String  @unique
  label   String?
  colorFg String
}